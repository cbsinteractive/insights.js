{
  "name": "insights.js",
  "version": "0.0.0",
  "main": "dist/lib.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build-development": "npm run clean && ./bin/build src/scout.js src/lib.js -v $npm_package_version",
    "build-production": "npm run clean && ./bin/build src/scout.js src/lib.js -p -v $npm_package_version",
    "build": "npm run verify && npm run build-development",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint src test",
    "verify": "npm run format && npm run lint",
    "unit-test": "karma start --single-run",
    "test": "npm run build && npm run unit-test"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-rewire-exports": "^0.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "5.3.0",
    "chai-datetime": "^1.5.0",
    "chai-http": "^3.0.0",
    "chai-spies": "^0.7.1",
    "electron": "^1.7.6",
    "eslint": "^3.16.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "fetch-mock": "^5.8.1",
    "karma": "^2.0.3",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "0.1.2",
    "karma-chai-spies": "^0.1.4",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-rollup-preprocessor": "^5.0.2",
    "karma-sauce-launcher": "^1.2.0",
    "mocha": "^3.2.0",
    "nomnom": "^1.8.1",
    "prettier": "^1.9.2",
    "rollup": "^0.51.8",
    "rollup-loader": "^0.1.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-conditional": "^1.1.1",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-livereload": "^0.4.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-post-replace": "^1.0.0",
    "rollup-plugin-serve": "^0.1.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "uuid": "^3.1.0"
  },
  "dependencies": {
    "core-js": "^2.5.1"
  }
}
